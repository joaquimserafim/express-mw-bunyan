    "logger",
    "bunyan",
    "services",
    "apis",
    "middleware"
  ],
  "author": "@JoaquimSerafim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaquimserafim/express-mw-bunyan/issues"
  },
  "homepage": "https://github.com/joaquimserafim/express-mw-bunyan#readme",
  "devDependencies": {
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.10",
    "chai": "^4.0.2",
    "coveralls": "^2.13.1",
    "express": "^4.15.3",
    "express-mw-correlation-id": "^3.0.0",
    "mocha": "^3.4.2",
    "pre-commit": "^1.2.2",
    "standard": "^10.0.2",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">=6.1"
  },
  "pre-commit": [
    "test",
    "coverage:check"
